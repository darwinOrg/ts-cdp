{
  "name": "ts-cdp",
  "version": "1.0.0",
  "description": "A TypeScript library for Chrome DevTools Protocol (CDP) operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "example": "ts-node examples/basic-usage.ts",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "server": "ts-node src/http/index.ts",
    "ws-server": "ts-node src/websocket/index.ts",
    "browser-controller": "ts-node src/http/browser-controller.ts"
  },
  "keywords": [
    "chrome",
    "cdp",
    "devtools-protocol",
    "puppeteer",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chrome-remote-interface": "^0.33.0",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/chrome-remote-interface": "^0.33.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
